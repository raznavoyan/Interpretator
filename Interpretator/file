def printVar (a) {
    print(a)|
}

a = 1|
c = 4|

if : a > c : {
    c = c + 1|
}  otherwise if : a < c {
    a = a + 1|
} otherwise {
    a = c + 2|
}

print(a)|
print(c)|

b = "hello"|
printVar(b)|

c = true|
d = 3.2|

array = [c, b, a, d>]

loop: i = 0 -> 4{
    print(array[i])|
}

f = 5|

during : f == true {
    f = f - 1;    
}
